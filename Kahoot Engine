javascript:fetch(`https://create.kahoot.it/rest/kahoots/${prompt("Paste the Kahoot ID")}/card/?includeKahoot=true`).then(res=%3Eres.json()).then(json=%3Ejson.kahoot.questions.map((q,number)=%3E{const{choices}=q;for(let%20i=0;i%3Cchoices.length;i+=1){if(choices[i].correct){switch(i){case%200:return%20`Q${number+1}:%20red%20triangle%20-%20${choices[i].answer%20}`;break;case%201:return%20`Q${number+1}:%20blue%20diamond%20-%20${choices[i].answer%20}`;break;case%202:return%20`Q${number+1}:%20yellow%20circle%20-%20${choices[i].answer%20}`;break;case%203:return%20`Q${number+1}:%20green%20square%20-%20${choices[i].answer%20}`;break}}}}).join(%22\n%22)).then(answers=%3Eopen().document.body.innerText=answers);
